{
	"name": "CopyPipeline_tsv_2_parquet",
	"properties": {
		"activities": [
			{
				"name": "ConvertCsvDataMy",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "raw/BingCoronavirusQuerySet/2020/"
					},
					{
						"name": "Destination",
						"value": "raw//2020_merged.parquet"
					}
				],
				"typeProperties": {
